package com.example.demo.form;

import javax.validation.constraints.NotBlank;

public class InsertItemForm {

	private String id;
	@NotBlank(message="error:may not be empty")
	private String name;
	@NotBlank(message="error:may not be empty")
	private String conditionId;
	@NotBlank(message="error:may not be empty")
	private String category;
	private String bigId;
	private String middleId;
	private String brand;
	private String price;
	private String shipping;
	private String description;

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getConditionId() {
		return conditionId;
	}

	public void setConditionId(String conditionId) {
		this.conditionId = conditionId;
	}

	public String getCategory() {
		return category;
	}

	public void setCategory(String category) {
		this.category = category;
	}

	public String getBigId() {
		return bigId;
	}

	public void setBigId(String bigId) {
		this.bigId = bigId;
	}

	public String getMiddleId() {
		return middleId;
	}

	public void setMiddleId(String middleId) {
		this.middleId = middleId;
	}

	public String getBrand() {
		return brand;
	}

	public void setBrand(String brand) {
		this.brand = brand;
	}

	public String getPrice() {
		return price;
	}

	public void setPrice(String price) {
		this.price = price;
	}

	public String getShipping() {
		return shipping;
	}

	public void setShipping(String shipping) {
		this.shipping = shipping;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	@Override
	public String toString() {
		return "InsertItemForm [id=" + id + ", name=" + name + ", conditionId=" + conditionId + ", category=" + category
				+ ", bigId=" + bigId + ", middleId=" + middleId + ", brand=" + brand + ", price=" + price
				+ ", shipping=" + shipping + ", description=" + description + "]";
	}

}
