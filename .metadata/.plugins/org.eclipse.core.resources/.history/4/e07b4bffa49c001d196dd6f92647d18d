package com.example.demo.domain;

import java.util.List;

/**
 * 親カテゴリーを表すドメイン.
 * 
 * @author inagakisaia
 *
 */
public class Big {

	/** ID */
	private int id;
	/** 名前 */
	private String name;

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	@Override
	public String toString() {
		return "Big [id=" + id + ", name=" + name + "]";
	}

}
